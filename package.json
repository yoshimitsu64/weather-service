{
  "name": "weather-service",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://yoshimitsu64.github.io/weather-service/",
  "dependencies": {
    "@react-oauth/google": "^0.5.0",
    "@redux-saga/testing-utils": "^1.1.5",
    "@reduxjs/toolkit": "^1.9.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/enzyme": "^3.10.12",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/redux-mock-store": "^1.0.3",
    "babel": "^6.23.0",
    "enzyme": "^3.11.0",
    "gh-pages": "^4.0.0",
    "react": "^18.2.0",
    "react-app-rewire-alias": "^1.1.7",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-elastic-carousel": "^0.11.5",
    "react-google-calendar-api": "1.5.2",
    "react-loader-spinner": "^5.3.4",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.1",
    "reselect": "^4.1.7",
    "styled-components": "^5.3.6",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "lint": "eslint --ext .ts, .tsx src --fix",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "test:cypress": "start-server-and-test start http://localhost:3000 cypress",
    "cypress": "cypress open",
    "cy:run": "cypress run"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "@store/(.*)": "<rootDir>/src/store/$1",
      "@api/(.*)": "<rootDir>/src/api/$1",
      "@components/(.*)": "<rootDir>/src/components/$1",
      "@constants/(.*)": "<rootDir>/src/constants/$1",
      "@containers/(.*)": "<rootDir>/src/containers/$1",
      "@helpers/(.*)": "<rootDir>/src/helpers/$1",
      "@hooks/(.*)": "<rootDir>/src/hooks/$1",
      "@interfaces/(.*)": "<rootDir>/src/interfaces/$1",
      "@styles/(.*)": "<rootDir>/src/styles/$1",
      "@appTypes/(.*)": "<rootDir>/src/appTypes/$1"
    },
    "setupFiles": [
      "<rootDir>/.env.local"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "verbose": false
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.4",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "cypress": "^12.0.2",
    "enzyme-adapter-react-16": "^1.15.7",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "start-server-and-test": "^1.15.2"
  }
}
